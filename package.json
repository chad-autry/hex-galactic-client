{
  "name": "hexagonal-space-client",
  "version": "0.0.0",
  "description": "The client for the hexagonal.space persistent programming mmo",
  "scripts": {
    "build": "mkdirp ./target/webapp && npm run less && npm run copy:HTML && webpack --config ./webpack-prod.config.js",
    "jshint": "jshint ./src/* --extract=auto --extra-ext=jsx",
    "clean": "rimraf ./target",
    "copy:HTML": "copyfiles -f ./src/index.html ./target/webapp",
    "less": "lessc ./src/less/main.less > ./target/webapp/styles.css",
    "server": "webpack-dev-server --content-base ./target/webapp --history-api-fallback --port 3000"
  },
  "keywords": [
    "hexagonal",
    "space",
    "mmo",
    "programming",
    "game"
  ],
  "author": {
    "name": "Chad Autry",
    "email": "chad.autry@gmail.com",
    "url": "http://chad-autry.github.io/"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "cartesian-hexagonal": "0.0.3",
    "client-auth-jwt": "^0.5.0",
    "data-chains": "^0.4.0",
    "hex-grid-map-3d": "^0.33.0",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "react-ace": "^4.1.1",
    "react-measure": "^1.4.5",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "copyfiles": "^0.2.1",
    "jshint": "^2.9.1-rc1",
    "less": "^2.5.3",
    "mkdirp": "^0.5.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "rimraf": "^2.4.4",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "jshintConfig": {
    "node": "true",
    "predef": [
      "document"
    ],
    "esversion": 6
  }
}
